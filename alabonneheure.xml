        titlehelp="À propos de la carte des horaires" 
		help="apps/cartoHoraires/html/help_horaires.html"
		showhelp="true"
		iconhelp=""
		style="apps/cartoHoraires/css/horaires.css"
		exportpng="true"
		geoloc="false"
		measuretools="false"
		stats="false"
		statsurl=""
